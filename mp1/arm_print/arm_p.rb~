t = 0
s = 0
r = 0
c = 0
puts "Enter the number upto which armstrong is to be generated"
n = gets.chomp.to_i
puts "The armstrong numbers from 1 to #{n} are"
for c in 1..n
  
  t = c
     while t > 0 do 
 	r = t%10
   	s = s+r*r*r
   	t = t/10
      end
   if s == c
      puts "#{c}"

     end
  s = 0
end
